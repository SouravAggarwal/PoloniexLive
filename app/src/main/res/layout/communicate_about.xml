<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.polosoft.sourav.poloniexlive.polosoft.polosoft.sourav.poloniexlive.Communicate.Abouts">

    <TextView
        android:id="@+id/textView31"
        android:layout_width="258dp"
        android:layout_height="39dp"
        android:text="polosofts@gmail.com"
        android:textSize="21sp"
        android:layout_marginRight="23dp"
        android:onClick="gmailclicked"
        android:textStyle="bold"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView32"
        tools:layout_editor_absoluteX="79dp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="34dp"
        android:layout_height="32dp"
        android:onClick="gmailclicked"

        app:srcCompat="@drawable/d_gmail"
        app:layout_constraintBottom_toBottomOf="@+id/textView31"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/textView31"
        android:layout_marginRight="0dp" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="313dp"
        android:layout_height="100dp"
        android:text="For any suggestion, feedback ,query or reporting any bug contact: "
        android:layout_marginLeft="8dp"
        android:textSize="16sp"
        android:gravity="center"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintHorizontal_bias="0.51" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="303dp"
        android:layout_height="37dp"
        android:text="Disclaimer: This app is not an official Application Developed by Poloniex"
        android:layout_marginRight="8dp"
        android:gravity="center"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="35dp" />

    <TextView
        android:id="@+id/myKeyNote"
        android:layout_width="317dp"
        android:layout_height="117dp"

        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.518"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/textView34"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView31" />

</android.support.constraint.ConstraintLayout>
